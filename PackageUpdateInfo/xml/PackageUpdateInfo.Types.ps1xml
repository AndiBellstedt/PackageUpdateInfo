<?xml version="1.0" encoding="utf-8"?>
<Types>

    <Type>
        <Name>PackageUpdate.Info</Name>
        <Members>
            <ScriptProperty>
                <Name>IsCurrentUserPath</Name>
                <GetScriptBlock>
                    (Split-Path -Path $this.Path -Parent) -in $script:CurrentUserModulePath
                </GetScriptBlock>
            </ScriptProperty>
        </Members>
    </Type>

</Types>